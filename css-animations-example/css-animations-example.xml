<!-- /wp:paragraph -->

<!-- wp:paragraph {"metadata":{"name":"Tutorial"}} -->
<p><strong><a href="#">Tutorial →</a></strong></p>
<!-- /wp:paragraph --></div>
<!-- /wp:group -->

<!-- wp:spacer {"height":"50px"} -->
<div style="height:50px" aria-hidden="true" class="wp-block-spacer"></div>
<!-- /wp:spacer -->

<!-- wp:group {"metadata":{"name":"Background Effect on Hover"},"layout":{"type":"constrained"}} -->
<div class="wp-block-group"><!-- wp:heading -->
<h2 class="wp-block-heading">Background Effect on Hover</h2>
<!-- /wp:heading -->

<!-- wp:paragraph -->
<p>Background turns light gray when hovering over it.</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"metadata":{"name":"Example"},"className":"is-style-default"} -->
<p class="is-style-default"><strong>Example:</strong></p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"className":"hover-bg is-style-default","style":{"border":{"radius":"10px","width":"1px"},"spacing":{"padding":{"top":"var:preset|spacing|20","bottom":"var:preset|spacing|20","left":"var:preset|spacing|20","right":"var:preset|spacing|20"}}},"borderColor":"accent-4"} -->
